		"rimraf": "^5.0.5",
		"supertest": "^6.3.4",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.4.2"
	},
